pipeline {
			agent {
					label {
								label "built-in"
								customWorkspace "/mnt/dock3"
							}
				}		
	stages {
		
		stage ('docker httpd') {
			
				steps {
			
				sh '''
						docker run -dp 8090:80 --name s3 httpd
						docker cp /mnt/dock1/index.html s3:/usr/local/apache2/htdocs/								
					
					'''
					}			
				}	
					
			}		
}
